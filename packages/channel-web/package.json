{
  "name": "@botpress/channel-web",
  "version": "10.33.0",
  "description": "An embeddable web chat for Botpress bots",
  "main": "dist/backend/index.js",
  "homepage": "https://github.com/botpress/modules",
  "botpress": {
    "menuText": "Web Chat",
    "menuIcon": "chrome_reader_mode",
    "webBundle": "bin/web.bundle.js",
    "noInterface": true,
    "plugins": [
      {
        "entry": "WebBotpressUIInjection",
        "position": "overlay"
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "postinstall": "npm run build",
    "test": "echo \"No tests\"",
    "watch": "concurrently \"node webpack.js --watch\" \"npm run watch-backend-js\" \"npm run watch-backend-ts\"",
    "watch-backend-js": "babel -w src/backend --out-dir dist/backend",
    "watch-backend-ts": "../../node_modules/.bin/tsc-watch -p .",
    "build": "node webpack.js --compile && npm run build-backend-js && npm run build-backend-ts",
    "build-backend-js": "babel src/backend --out-dir dist/backend",
    "build-backend-ts": "../../node_modules/.bin/rimraf dist && ../../node_modules/.bin/tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/botpress/modules"
  },
  "babel": {
    "plugins": [
      "transform-object-rest-spread",
      "transform-decorators-legacy",
      [
        "inline-import",
        {
          "extensions": [
            ".raw.js"
          ]
        }
      ]
    ],
    "presets": [
      [
        "es2015",
        {
          "targets": {
            "node": "6.10"
          }
        }
      ]
    ]
  },
  "author": "Botpress",
  "license": "AGPL-3.0-only",
  "peerDependencies": {
    "botpress": ">= 10.0.0"
  },
  "devDependencies": {
    "@types/bluebird-global": "^3.5.7",
    "@types/lodash": "^4.14.116",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.24.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "classnames": "^2.2.5",
    "concurrently": "^3.6.1",
    "copy-webpack-plugin": "^4.3.1",
    "core-js": "^2.4.1",
    "css-loader": "^0.28.8",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "image-webpack-loader": "^4.3.1",
    "node-sass": "^3.11.2",
    "raw-loader": "^0.5.1",
    "react-chatview": "^0.2.5",
    "react-dropzone": "^3.13.1",
    "react-linkify": "^0.2.2",
    "react-resizable-box": "^2.0.4",
    "react-slick": "^0.15.4",
    "react-spinkit": "^3.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "slick-carousel": "^1.8.1",
    "snarkdown": "^1.2.2",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "aws-sdk": "^2.108.0",
    "axios": "^0.16.2",
    "bluebird": "^3.5.1",
    "bluebird-global": "^1.0.1",
    "botpress-module-sdk": "file:../module-sdk",
    "date-fns": "^1.28.5",
    "lodash": "^4.17.4",
    "lodash-decorators": "^6.0.0",
    "mime": "^2.0.3",
    "moment": "^2.18.1",
    "ms": "^2.1.1",
    "multer": "^1.3.0",
    "multer-s3": "^2.7.0",
    "query-string": "^5.0.1",
    "react-ga": "^2.4.1",
    "serve-static": "^1.12.3",
    "sillyname": "^0.1.0",
    "uuid": "^3.1.0"
  }
}
