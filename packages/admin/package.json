{
  "name": "@botpress/xx-admin",
  "version": "1.0.0",
  "description": "Botpress Admin Panel",
  "main": "dist",
  "engines": {
    "node": "^8.9.4"
  },
  "scripts": {
    "watch": "concurrently --kill-others --names \"front\" \"cd front && npm start\"",
    "build-front": "cd front && npm install && npm run build && cp -r build ../back/static",
    "build": "npm run build-front && cp -r front/build ../ui/static/admin"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "Botpress, Inc.",
  "license": "ISC",
  "dependencies": {
    "@botpress/util-roles": "^10.36.1",
    "@botpress/xx-util": "file:../util",
    "apicache": "^1.2.1",
    "auth0": "^2.9.1",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "express": "^4.13.3",
    "express-force-https": "^1.0.0",
    "express-jwt": "^5.3.1",
    "joi": "^13.1.2",
    "jwks-rsa": "^1.2.1",
    "ldapauth-fork": "^4.0.2",
    "moment": "^2.21.0",
    "morgan": "^1.8.0",
    "ms": "^2.1.1",
    "nanoid": "^1.0.2",
    "pem-jwk": "^1.5.1",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "resource-router-middleware": "^0.6.0",
    "sequelize": "^4.36.1",
    "umzug": "^2.1.0",
    "yn": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-root-import": "^6.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "nodemon": "^1.9.2",
    "prettier": "^1.13.4"
  }
}
