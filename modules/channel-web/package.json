{
  "name": "botpress-channel-web",
  "version": "0.0.0",
  "author": "Botpress",
  "license": "AGPL-3.0-only",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "serve": "node dist/src/server.js",
    "build": "npm run build-ts && npm run tslint && ./copyStaticFiles.sh",
    "build-ts": "tsc",
    "build-pkg": "npm run build && npm run pkg",
    "pkg": "pkg dist/src/server.js --target macos-x86,macos-x64,linux-x86,linux-x64,win-x86,win-x64 --output bin/botpress",
    "test": "jest --forceExit --coverage --verbose",
    "watch": "npm run watch-ts",
    "watch-ts": "tsc-watch -p . --onSuccess \"node ./dist/src/server.js\" --onFailure \"echo Beep! Compilation Failed\"",
    "watch-node": "nodemon dist/src/server.js -w dist",
    "watch-test": "npm run test -- --watchAll",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "dependencies": {
    "botpress-module-sdk": "../../packages/module-sdk",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.0",
    "@types/node": "^10.5.2",
    "concurrently": "^3.6.0",
    "errorhandler": "^1.5.0",
    "jest": "^23.4.1",
    "nodemon": "^1.18.2",
    "pkg": "^4.3.3",
    "prettier": "1.13.7",
    "ts-jest": "^23.0.0",
    "ts-node": "^7.0.0",
    "tsc-watch": "^1.0.26",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  }
}
